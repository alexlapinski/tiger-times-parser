    "email",
    "newsletter"
  ],
  "author": "Alex Lapinski <contact@alexlapinski.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexlapinski/tiger-times-parser/issues"
  },
  "homepage": "https://github.com/alexlapinski/tiger-times-parser#readme",
  "devDependencies": {
    "@types/node": "^12.7.5",
    "@types/ramda": "^0.26.21",
    "@types/rc": "^1.1.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "googleapis": "^51.0.0",
    "ramda": "^0.26.1",
    "rc": "^1.2.8",
    "readline": "^1.3.0"
  }
