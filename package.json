  "description": "A simple tool to parse the email newsletter sent by our Elementary School (the 'Tiger Times')",
  "main": "./lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "npm -s run build && node ./lib/index.js",
    "debug": "npm -s run build && node --nolazy --inspect-brk=9229 ./lib/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexlapinski/tiger-times-parser.git"
  },
  "keywords": [
    "parser",
